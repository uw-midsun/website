{
  "name": "UWMIDSUN",
  "version": "1.0.0",
  "description": "University of Waterloo Midnight Sun Solar Car Team Official Website",
  "main": "src/server.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uw-midsun/website.git"
  },
  "scripts": {
    "start": "NODE_ENV=production node_modules/.bin/babel-node --presets 'react,es2015' src/server.js",
    "start-dev": "npm run start-dev-hmr",
    "start-dev-single-page": "node_modules/.bin/http-server src/static",
    "start-dev-hmr": "node_modules/.bin/webpack-dev-server --hot --progress --inline --hot",
    "build": "node_modules/.bin/webpack -p",
    "test": "./node_modules/.bin/eslint src !src/static/js"
  },
  "author": "UWMIDSUN",
  "license": "",
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "ejs": "^2.5.2",
    "express": "^4.14.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-router": "^2.8.1"
  },
  "devDependencies": {
    "eslint": "^3.9.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-promise": "^3.3.1",
    "eslint-plugin-react": "^6.5.0",
    "eslint-plugin-standard": "^2.0.1",
    "http-server": "^0.9.0",
    "react-hot-loader": "^1.3.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.14.1"
  }
